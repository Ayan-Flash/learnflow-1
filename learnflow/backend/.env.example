# Server
NODE_ENV=development
PORT=8080

# Google Gemini
GOOGLE_API_KEY=
GEMINI_MODEL=gemini-1.5-flash

# Weights & Biases
WANDB_API_KEY=
WANDB_PROJECT=learnflow-ai
WANDB_ENTITY=

# Logging
LOG_LEVEL=info

# Dashboard telemetry (Phase 8)
# Used only for anonymized aggregation. Do not put secrets here; use a random string.
ANONYMIZATION_SALT=change-me
TELEMETRY_FILE=./data/telemetry.jsonl

# Optional Gemini cost estimation (USD)
GEMINI_COST_PER_1K_INPUT_TOKENS=0.00025
GEMINI_COST_PER_1K_OUTPUT_TOKENS=0.0005
